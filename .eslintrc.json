{
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "env": {
    "es6": true,
    "browser": true,
    "commonjs": true,
    "node": true
  },
  "extends": "prettier",
  "plugins": ["prettier"],
  "rules": {
    "array-callback-return": "error",
    "camelcase": "error",
    "curly": "error",
    "dot-notation": "error",
    "eqeqeq": "error",
    "no-duplicate-case": "error",
    "no-empty": "error",
    "no-empty-function": "error",
    "no-func-assign": "error",
    "no-unreachable": "error",
    "quotes": ["error", "double"],
    "yoda": "error"
  }
}
